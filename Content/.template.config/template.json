{
  "$schema": "http://json.schemastore.org/template",
  "author": "Tomasz Heimowski",
  "classifications": [ "F#", "Web", "Suave", "Fable", "Elmish" ],
  "name": "SAFE-Stack Web App v0.2.2",
  "tags": {
    "language": "F#",
    "type": "project"
  },
  "identity": "SAFE.Template",
  "shortName": "SAFE",
  "symbols": {
    "Fulma": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false",
      "description": "adds Fulma (Fable bindings for Bulma.io) to client"
    },
    "Remoting": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false",
      "description": "adds Fable.Remoting to server and client"
    }
  },
  "SpecialCustomOperations": {
    "**/paket.*": {
      "operations": [
        {
          "type": "conditional",
          "configuration": {
            "if": [ "#if" ],
            "else": [ "#else" ],
            "elseif": [ "#elseif" ],
            "endif": [ "#endif" ],
            "trim": "true",
            "wholeLine": "true"
          }
        }
      ]
    }
  }
}